{
    "name": "ericfortmeyer/jwvirtualassistant_login_create-account",
    "type": "library",
    "description": "Use to create accounts for jwvirtualassistant",
    "require": {
        "vlucas/phpdotenv": "^3.4",
        "zendframework/zend-crypt": "^3.3",
        "guzzlehttp/psr7": "^1.6",
        "narrowspark/http-emitter": "^0.8.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Eric Fortmeyer",
            "email": "e.fortmeyer01@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "jwvirtualassistant\\AccountManagement\\": "./"
        },
        "files": [
            "functions.php",
            "loadEnvVars.php",
            "constants.php",
            "redirectToErrorPage.php"
        ]
    },
    "require-dev": {
        "phan/phan": "^2.2",
        "squizlabs/php_codesniffer": "^3.4"
    }
